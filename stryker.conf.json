{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "mutator": "typescript",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress"
  ],
  "testRunner": "jest",
  "babel": {
    "optionsFile": "babel.config.js"
  },
  "transpilers": [
    "babel"
  ],
  "coverageAnalysis": "off",
  "tsconfigFile": "tsconfig.json",
  "mutate": [
    "src/**/*.ts",
    "!src/server.ts",
    "!test/**/*.spec.ts"
  ]
}